.index-page{
  width: 100%;
  display: flex;
  flex-direction: column;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 50px;
}
.toolbar-root{
  flex-shrink: 0;
}
.index-content{
  flex-grow: 1;
  height: 100%;
  overflow: hidden;
}
.swiper-container{
  height: 160px;
  width: 100%;
  top: 0;
  position: relative;
}
.img-wrapper{
  width: 100%;
  height: 100%;
}
.swip-out{
  position: absolute;
  top: 5px;
  left: 0;
  width: 94%;
  border-radius: 15px;
  overflow: hidden;
  transform: translateX(3%);
}
.brand-list-wrapper{
  box-sizing: border-box;
  position: relative;
  height: 340rpx;
  padding: 10px;
}
.brand-list-wrapper>.list-contanier{
  background-color: white;
  border-radius: 15px;
  position: relative;
  height: 100%;
  overflow: hidden;
}
.list-contanier>.title{
  line-height: 60rpx;
  height: 60rpx;
  font-weight: 700;
  font-size: 12px;
  position: relative;
  padding-left: 20px;
}
.list-contanier>.title::before{
  content: '';
  position: absolute;
  top: 15rpx;
  left: 10px;
  width: 3px;
  height: 30rpx;
  background-color: var(--themeColor);
}
.brand-contaniner{
  padding: 10px 0;
  display: flex;
  overflow-x: auto;
  position: absolute;
  top: 60rpx;
  left: 0;
  right: 0;
  bottom: -10px;
}
.brand-item{
  width: 200rpx;
  height: 200rpx;
  flex-shrink: 0;
  display: flex;
  flex-direction: column;
}
.brand-item>image{
  width: 150rpx;
  height: 150rpx;
  padding-left: 25rpx;
}
.brand-item>text{
  font-size: 12px;
  line-height: 50rpx;
  height: 50rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  text-align: center;
}
