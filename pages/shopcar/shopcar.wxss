#shopcar-warrper {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 100px;
  display: flex;
  flex-direction: column;
}
.tool-wrapper {
  flex-shrink: 0;
}
.shopcar-content {
  flex: 1;
  height: 100%;
  display: flex;
  border-top: 1px solid #ddd;
  width: 100%;
}
.shopcar-page {
  height: 100%;
  flex-grow: 1;
}
.shopcar-srcoll {
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.shopcar-scroll-content {
  height: 100%;
  width: 100%;
}
.address-edit-pro {
  padding: 0 15px;
  background-color: white;
  border-bottom-left-radius: 15px;
  border-bottom-right-radius: 15px;
  font-size: 14px;
  line-height: 40px;
  display: flex;
  justify-content: space-between;
  font-size: 16px;
}
.icon-wrapper {
  font-size: 16px;
  padding-left: 20rpx;
}
.icon-wrapper > i {
  font-size: 16px;
  color: #333;
  padding-right: 10px;
}
.edit-pro:before {
  content: '';
  margin-right: 20px;
  border-left: 3px solid #ddd;
}

.pro-list-wrapper {
  border-radius: 15px;
  background-color: white;
}
.list-top {
  height: 40px;
  margin-top: 10px;
  font-size: 14px;
  position: relative;
}
.check-icon {
  width: 50px;
  height: 100%;
  position: relative;
  position: absolute;
  top: 0;
}
.check-icon:after {
  position: absolute;
  top: 50%;
  left: 5px;
  transform: translateY(-50%);
  content: '';
  background-image: url('data:image/png;base64,');
  background-size: 30px 30px;
  width: 30px;
  height: 30px;
}
.select-check:after {
  content: '';
  background-image: url('data:image/png;base64,');
  background-size: 30px 30px;
  width: 30px;
  height: 30px;
}
.jd-zy {
  font-weight: 700;
  line-height: 40px;
  padding-left: 50px;
}
.discount {
  float: right;
  line-height: 20px;
  padding: 0 4px;
  font-size: 12px;
  border-radius: 10px;
  color: var(--themeColorLight);
  background-color: #fcedeb;
  margin: 10px 10px;
}
.pro-list-item {
  border-bottom: 1px solid #ddd;
}
.pro-list-item:last-child {
  border: 0;
}
.pro-list-item-content {
  padding: 6px 0;
  display: flex;
  box-sizing: border-box;
  font-size: 16px;
  width: 100%;
}
.check-wrapper {
  position: relative;
  flex-shrink: 0;
  min-width: 30px;
}
.check-wrapper > text {
  padding-left: 40px;
}
.check-icon:after {
  position: absolute;
  top: 50%;
  left: 5px;
  transform: translateY(-50%);
  content: '';
  background-image: url('data:image/png;base64,');
  background-size: 30px 30px;
  width: 30px;
  height: 30px;
}
.select-check:after {
  position: absolute;
  top: 50%;
  left: 5px;
  transform: translateY(-50%);
  content: '';
  background-image: url('data:image/png;base64,');
  background-size: 30px 30px;
  width: 30px;
  height: 30px;
  border-radius: 50%;
}
.pro-img {
  flex-shrink: 0;
  width: 90px;
  height: 90px;
}
.pro-info {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 10px 5px;
  flex-grow: 1;
  overflow: hidden;
}
.pro-info > .title {
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
  font-size: 14px;
  font-weight: 600;
  line-height: 22px;
}

.price-count {
  line-height: 40px;
  justify-content: space-between;
  display: flex;
  width: 100%;
}
.count-wrapper {
  width: 100px;
}
.price-count > .price {
  color: var(--themeColorLight);
  font-size: 16px;
  font-weight: 700;
  flex-shrink: 0;
  display: inline-block;
}
.price > .large_size {
  font-size: 20px;
}
.price_decimals {
  font-size: 16px;
}
.attation-del {
  height: 30px;
  font-size: 14px;
  padding-right: 15px;
}
.attation-del-content {
  float: right;
}
.attation-del-content > text {
  padding: 0 10px;
}
.attation-del-content > text:last-child {
  border-left: 1px solid #ddd;
}

.empty-root {
  width: 100%;
  height: 100%;
}
.login-wraper {
  padding: 10px;
  text-align: center;
  font-size: 14px;
  background-color: white;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
}
.btn-login {
  display: inline-block;
  margin-left: 10px;
  width: 58px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  color: #fff;
  background-image: -webkit-linear-gradient(right, #f2270c, #ff4b2b);
  background-image: linear-gradient(270deg, #f2270c, #ff4b2b);
  box-shadow: 0 3px 6px 0 rgba(255, 65, 66, 0.2);
  border-radius: 15px;
}

.cart-bottom {
  background-color: white;
  line-height: 50px;
  width: 100%;
  height: 50px;
  position: fixed;
  bottom: 50px;
  left: 0;
  display: flex;
  justify-content: space-between;
}
.select-check:after {
  position: absolute;
  top: 50%;
  left: 5px;
  transform: translateY(-50%);
  content: '';
  background-image: url('data:image/png;base64,');
  background-size: 30px 30px;
  width: 30px;
  height: 30px;
  border-radius: 50%;
}

.right {
  display: flex;
  font-size: 16px;
  padding-right: 15px;
}
.total-price-wrapper {
  padding-right: 10px;
}
.total-price-wrapper > text:first-child {
  color: #333;
}
.total-price-wrapper > text:last-child {
  color: black;
  font-weight: 700;
}

.btn-pay {
  line-height: 40px;
  height: 40px;
  margin-top: 5px;
  border-radius: 20px;
  padding: 0 10px;
  color: white;
  font-weight: 700;
  font-size: 14px;
  background-image: -webkit-linear-gradient(
    left,
    #f10000,
    #ff2000 73%,
    #ff4f18
  );
  background-image: -o-linear-gradient(left, #f10000, #ff2000 73%, #ff4f18);
  background-image: linear-gradient(90deg, #f10000, #ff2000 73%, #ff4f18);
}
.right-edit {
  display: flex;
  font-size: 16px;
  padding-right: 15px;
}
.right-edit > text {
  line-height: 30px;
  height: 30px;
  margin-top: 10px;
  border-radius: 15px;
  padding: 0 10px;
  color: #666;
  font-size: 14px;
  margin-right: 10px;
  border: 1px solid #999;
}
